
   # Indented comment

# All tests with a port appear first, then all tests without a port. Makes tests
# easier to write.

# Old-style mappings
127.0.0.1:192.168.0.1:1001
127.0.0.2:192.168.0.2:1002# End of line comment

# New-style mappings with IPv4 mapping source
127.0.1.1 192.168.1.1 1001
127.0.1.2 192.168.1.2 1002# End of line comment

127.0.2.1 ::ffff:192.168.2.1 1001
127.0.2.2 0000:0000:0000:0000:0000:ffff:192.168.2.2 1002
127.0.2.3 0:0:0:0:0:ffff:192.168.2.3 1003

127.0.3.1 ::1 1001
127.0.3.2 0000:0000:0000:0000:0000:0000:0000:0002 1002
127.0.3.3 0:0:0:0:0:0:0:3 1003

# New-style mappings with IPv6 fully-specified mapping source
0000:0000:0000:0000:0000:FFFF:7F00:0401 192.168.4.1 1001

0000:0000:0000:0000:0000:FFFF:7F00:0501 ::ffff:192.168.5.1 1001
0000:0000:0000:0000:0000:FFFF:7F00:0502 0000:0000:0000:0000:0000:ffff:192.168.5.2 1002
0000:0000:0000:0000:0000:FFFF:7F00:0503 0:0:0:0:0:ffff:192.168.5.3 1003

0000:0000:0000:0000:0000:FFFF:7F00:0601 ::1 1001
0000:0000:0000:0000:0000:FFFF:7F00:0602 0000:0000:0000:0000:0000:0000:0000:0002 1002
0000:0000:0000:0000:0000:FFFF:7F00:0603 0:0:0:0:0:0:0:3 1003

# New-style mappings with IPv6 shortened mapping source
0:0:0:0:0:FFFF:7F00:0701 192.168.7.1 1001

0:0:0:0:0:FFFF:7F00:0801 ::ffff:192.168.8.1 1001
0:0:0:0:0:FFFF:7F00:0802 0000:0000:0000:0000:0000:ffff:192.168.8.2 1002
0:0:0:0:0:FFFF:7F00:0803 0:0:0:0:0:ffff:192.168.8.3 1003

0:0:0:0:0:FFFF:7F00:0901 ::1 1001
0:0:0:0:0:FFFF:7F00:0902 0000:0000:0000:0000:0000:0000:0000:0002 1002
0:0:0:0:0:FFFF:7F00:0903 0:0:0:0:0:0:0:3 1003

# New-style mappings with IPv6 truncated mapping source
::FFFF:7F00:0a01 192.168.10.1 1001

::FFFF:7F00:0b01 ::ffff:192.168.11.1 1001
::FFFF:7F00:0b02 0000:0000:0000:0000:0000:ffff:192.168.11.2 1002
::FFFF:7F00:0b03 0:0:0:0:0:ffff:192.168.11.3 1003

::FFFF:7F00:0c01 ::1 1001
::FFFF:7F00:0c02 0000:0000:0000:0000:0000:0000:0000:0002 1002
::FFFF:7F00:0c03 0:0:0:0:0:0:0:3 1003

# New-style mappings with IPv6 v4-compatibility mapping source
::FFFF:127.0.13.1 192.168.13.1 1001

::FFFF:127.0.14.1 ::ffff:192.168.14.1 1001
::FFFF:127.0.14.2 0000:0000:0000:0000:0000:ffff:192.168.14.2 1002
::FFFF:127.0.14.3 0:0:0:0:0:ffff:192.168.14.3 1003

::FFFF:127.0.15.1 ::1 1001
::FFFF:127.0.15.2 0000:0000:0000:0000:0000:0000:0000:0002 1002
::FFFF:127.0.15.3 0:0:0:0:0:0:0:3 1003


# All previous tests, without port numbers.

# Old-style mappings
127.1.0.1:192.168.0.1
127.1.0.2:192.168.0.2# End of line comment

